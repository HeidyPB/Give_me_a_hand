
 <div class="card-friend">
    <img src="data:image/jpeg;base64," />
    <div class="card-friend-infos">
      <h3>Friend <%= @friend.username %></h1>
      <h3><%= @friend.origin %></h1>
      <h3><%= @friend.province %></h1>
      <h3><%= @friend.languages.first %></h1>
      <h3><%= @friend.categories.first %></h1>
      <p>I can help you with <strong>open bank accounts</strong>.</p>
    </div>
  </div>
  <br>
  <br>

    <h2><strong>What friends say about Lucie</strong></h2>
    <div class="row">
      <div class="col-lg-3">
        <ul class="list-group">
          <% @friend.reviews_as_friend.each do |review| %>
            <br>
            <li class="list-group-item"><%= 'avatar' %></li>
            <li class="list-group-item"><%= review.commentary %></li>
            <li class="list-group-item"><%= '⭐️' * review.rating %></li>
            <li class="list-group-item"><%= 'username' %></li>
          <% end %>
        </ul>
      </div>
    </div>
